<style>
    #usuarioalterarSenha {
        padding-right: 0px !important;
    }
</style>





<div class="modal right fade" id="usuarioalterarDados" tabindex="-1" role="dialog" aria-labelledby="myModalLabel2" style="padding-right: 0px !important" data-backdrop="static">
    <div class="modal-dialog" role="document">
        <div class="modal-content">

            <div class="modal-header">
                <h4 class="modal-title" id="myModalLabel2">Alterar Usuário</h4>
                <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
            </div>
            <input id="usu_id_modalalt" name="usu_id_modalalt" type="hidden" class="form-control obrigatorio" required>
            <div class="modal-body">
                <div class="col-sm-12">
                    <div class="form-cli_nomeFantasia">
                        <label for="usu_nomemodalalt" class="control-label mb-1">Nome *</label>
                        <div class="nk-int-st">
                            <input id="usu_nomemodalalt" name="usu_nomemodalalt" type="text" placeholder="Nome Fantasia" class="form-control obrigatorio" required>
                        </div>
                    </div>
                </div>
                <div class="col-sm-12">
                    <div class="form-group">
                        <label for="usu_telefonemodalat" class="control-label mb-1">Telefone *</label>
                        <div class="nk-int-st">
                            <input id="usu_telefonemodalat" name="usu_telefonemodalat" type="text" placeholder="Razão Social" class="form-control formCel obrigatorio" required>
                        </div>
                    </div>
                </div>
                <div class="col-sm-12">
                    <div class="form-group">
                        <label for="usu_celularmodalalt" class="control-label mb-1">Celular *</label>
                        <div class="nk-int-st">
                            <input id="usu_celularmodalalt" name="usu_celularmodalalt" type="text" placeholder="Razão Social" class="form-control formCel obrigatorio" required>
                        </div>
                    </div>
                </div>
                <div class="col-sm-12">
                    <label for="usu_senha" class="control-label mb-1">Imagem</label><br />
                    <label class=fileuploadnewbtn>
                        <img id="imageusu_imagemodalalt" src="http://placehold.it/120x120">
                        <input id="file" class="fileuploadimagem" onchange="SalvarImagemServidor(this, 'Usuario', 'imageusu_imagemodalalt', 'usu_imagemodalalt');" type="file">
                        <input id="usu_imagemodalalt" name="usu_imagemodalalt" type="hidden" class="form-control">
                    </label>
                </div>

               

                <div class="col-sm-12">
                    <div class="row">
                        <div class="col-sm-6">
                            <div class="row">
                                <div class="col-sm-6">
                                    <button onclick="CancelarCadastroCliente()" class="btn btn-default text-left">
                                        <i class="fa fa-ban"></i> Cancelar
                                    </button>
                                </div>
                                <div class="col-sm-6">
                                    <button onclick="AlterarDadosUsuario()" class="btn btn-success text-right rotacionaricone">
                                        <div class="iconebutton" style="float: left; margin-right: 5px">
                                            <i class="fa fa-spinner"></i>
                                        </div>
                                        Salvar
                                    </button>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>


<div class="modal fade" id="usuarioalterarSenha" role="dialog">
    <div class="modal-dialog modals-default">
        <div class="modal-content">
            <div class="modal-header" style="margin-bottom: 20px;background-color: #fff !important;">
                <div class="row">
                    <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
                        <div class="breadcomb-wp">
                            <div class="breadcomb-icon">
                                <i class="notika-icon notika-app"></i>
                            </div>
                            <div class="breadcomb-ctn">
                                <h2>Usuário</h2>
                                <p>Alterar Senha</p>
                            </div>
                        </div>
                    </div>
                </div>
                <button type="button" class="close" data-dismiss="modal">&times;</button>
            </div>
            <div class="modal-body">
                <input id="usu_idmodal" name="usu_idmodal" type="hidden" value="" />
                <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
                    <div class="form-group">
                        <label for="mod_nome" class="control-label mb-1">Nova Senha</label>
                        <div class="nk-int-st">
                            <input id="usu_senhamodal" placeholder="Nome do Perfil" name="usu_senhamodal" type="password" value="" class="form-control obrigatorio">
                        </div>

                    </div>
                </div>
                <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
                    <div class="form-group">
                        <label for="mod_nome" class="control-label mb-1">Confirmação Senha</label>
                        <div class="nk-int-st">
                            <input id="usu_senhaconfirmacaomodal" placeholder="Nome do Perfil" name="usu_senhaconfirmacaomodal" type="password" value="" class="form-control obrigatorio">
                        </div>

                    </div>
                </div>
            </div>
            <div class="modal-footer" style="margin-top: 30px">
                <button type="button" class="btn btn-default" onclick="AlterarSenhaUsuario()" data-dismiss="modal">Salvar</button>
                <button type="button" class="btn btn-default" data-dismiss="modal">Cancelar</button>
            </div>
        </div>
    </div>
</div>