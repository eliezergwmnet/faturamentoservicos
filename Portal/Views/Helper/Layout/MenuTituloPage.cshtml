@{ 
    string iconTituloPage = "";
    string tituloTituloPage = "";
    string descricaoTituloPage = "";
    bool buttonInsert = false;
    buttonInsert = ViewBag.ButtonInsert == null || ViewBag.ButtonInsert == "" ? false : Convert.ToBoolean(ViewBag.ButtonInsert);
    iconTituloPage = ViewBag.IconPage == null || ViewBag.IconPage == "" ? "notika-app" : ViewBag.IconPage;
    tituloTituloPage = ViewData[Globais.Helper.TituloPageEnum.TituloPagina.ToString()] == null ? "Titulo da Pagina" : ViewData[Globais.Helper.TituloPageEnum.TituloPagina.ToString()].ToString();
    descricaoTituloPage = ViewData[Globais.Helper.TituloPageEnum.TituloModuloPagina.ToString()] == null  ? "" : ViewData[Globais.Helper.TituloPageEnum.TituloModuloPagina.ToString()].ToString();
}
<div class="breadcomb-area">
    <div class="container">
        <div class="row">
            <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
                <div class="breadcomb-list">
                    <div class="row">
                        <div class="col-lg-6 col-md-6 col-sm-6 col-xs-12">
                            <div class="breadcomb-wp">
                                <div class="breadcomb-icon">
                                    <i class="notika-icon @iconTituloPage"></i>
                                </div>
                                <div class="breadcomb-ctn">
                                    <h2>@tituloTituloPage</h2>
                                    <p>@descricaoTituloPage</p>
                                </div>
                            </div>
                        </div>
                        <div class="col-lg-6 col-md-6 col-sm-6 col-xs-3" id="divPagesCadastro">
                            @if (buttonInsert == false) {
                                <div class="breadcomb-report">
                                    <button data-toggle="tooltip" id="buttonTituloPagesCadastro" data-placement="left" title="" class="btn waves-effect" data-original-title="Cadastrar Novo"><i class="notika-icon notika-sent"></i></button>
                                </div>
                            }
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>