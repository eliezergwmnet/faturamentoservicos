        <script src="@Url.Content("~/Content/Template/Verde/js/jvectormap/jquery-jvectormap-world-mill-en.js")"></script>
        <script src="@Url.Content("~/Content/Template/Verde/js/jvectormap/jvectormap-active.js")"></script>
        <script src="@Url.Content("~/Content/Template/Verde/js/sparkline/jquery.sparkline.min.js")"></script>
        <script src="@Url.Content("~/Content/Template/Verde/js/sparkline/sparkline-active.js")"></script>
        <script src="@Url.Content("~/Content/Template/Verde/js/flot/jquery.flot.js")"></script>
        <script src="@Url.Content("~/Content/Template/Verde/js/flot/jquery.flot.resize.js")"></script>

