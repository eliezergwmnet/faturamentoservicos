        <script src="@Url.Content("~/Content/Template/Verde/js/wave/wave-active.js")"></script>
        <script src="@Url.Content("~/Content/Template/Verde/js/todo/jquery.todo.js")"></script>
        <script src="@Url.Content("~/Content/Template/Verde/js/plugins.js")"></script>
        <script src="@Url.Content("~/Content/Template/Verde/js/chat/moment.min.js")"></script>
        <script src="@Url.Content("~/Content/Template/Verde/js/chat/jquery.chat.js")"></script>

        <script src="@Url.Content("~/Content/Template/Verde/js/dropzone/dropzone.js")"></script>

        <script src="@Url.Content("~/Content/js/jquery.maskMoney.min.js")"></script>
        <script src="@Url.Content("~/Content/Template/Verde/js/bootstrap-select/bootstrap-select.js")"></script>

        <script src="@Url.Content("~/Content/Template/Verde/js/main.js")"></script>
