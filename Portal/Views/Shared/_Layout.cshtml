        <script src="@Url.Content("~/Content/Template/Verde/js/counterup/counterup-active.js")"></script>
        <script src="@Url.Content("~/Content/Template/Verde/js/scrollbar/jquery.mCustomScrollbar.concat.min.js")"></script>
