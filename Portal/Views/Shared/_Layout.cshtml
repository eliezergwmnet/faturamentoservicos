    <div class="footer-copyright-area">
        <div class="container">
            <div class="row">
                <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
                    <div class="footer-copy-right">
                        <p>
                            Registrado © @DateTime.Now.Year
                            . Todos os direitos reservados, desenvolvido por <a href="http://sistemaviaweb.com.br">Sistema Via Web</a>.
                        </p>
                    </div>
                </div>
            </div>
        </div>
    </div>
    @Html.Partial("~/Views/Helper/PartialGlobais/Index.cshtml")
