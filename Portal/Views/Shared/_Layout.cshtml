        <script src="@Url.Content("~/Content/js/DateFormat.js")"></script>

        <script src="@Url.Content("~/Content/Template/Verde/js/flot/curvedLines.js")"></script>
        <script src="@Url.Content("~/Content/Template/Verde/js/flot/flot-active.js")"></script>
        <script src="@Url.Content("~/Content/Template/Verde/js/knob/jquery.knob.js")"></script>
        <script src="@Url.Content("~/Content/Template/Verde/js/knob/jquery.appear.js")"></script>
        <script src="@Url.Content("~/Content/Template/Verde/js/knob/knob-active.js")"></script>
