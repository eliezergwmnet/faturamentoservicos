        <script src="@Url.Content("~/Content/js/main.js")"></script>
        <script src="@Url.Content("~/Content/js/Projeto/Globais/Contato.js")"></script>
        <script src="@Url.Content("~/Content/js/Projeto/Globais/Endereco.js")"></script>
        <script src="@Url.Content("~/Content/js/Projeto/Globais/Alertas.js")"></script>

        <script src="@Url.Content("~/Content/Template/Verde/js/icheck/icheck.min.js")"></script>


