    <div class="header-top-area">
        <div class="container">
            <div class="row">
                <div class="col-lg-4 col-md-4 col-sm-12 col-xs-12">
                    <div class="logo-area">
                        <a href="@Url.Content("~/")"><img src="@HelpSistemaModel.Configuracao().Logo" alt="@HelpSistemaModel.Configuracao().Titulo" style="max-height: 30px;"/></a>
                    </div>
                </div>
                @Html.Partial("~/Views/Helper/Layout/MenuTopNotificacao.cshtml")
                
            </div>
        </div>
    </div>
    <!-- End Header Top Area -->
    @Html.Partial("~/Views/Helper/Layout/MenuLateral.cshtml")
    <!-- Start Status area -->
    <div class="notika-status-area">

        @Html.Partial("~/Views/Helper/Layout/MenuTituloPage.cshtml")
        @Html.Partial("~/Views/Helper/PartialGlobais/AlertasPartial.cshtml")
        @Html.Partial("~/Views/Helper/PartialGlobais/LoadingPartial.cshtml")

        <script src="@Url.Content("~/Content/Template/Verde/js/vendor/jquery-1.12.4.min.js")"></script>
        <script src="@Url.Content("~/Content/Template/Verde/js/bootstrap.min.js")"></script>
        <script src="@Url.Content("~/Content/Template/Verde/js/wow.min.js")"></script>
        <script src="@Url.Content("~/Content/Template/Verde/js/jquery-price-slider.js")"></script>
        <script src="@Url.Content("~/Content/Template/Verde/js/owl.carousel.min.js")"></script>
        <script src="@Url.Content("~/Content/Template/Verde/js/jquery.scrollUp.min.js")"></script>
        <script src="@Url.Content("~/Content/Template/Verde/js/meanmenu/jquery.meanmenu.js")"></script>
        <script src="@Url.Content("~/Content/Template/Verde/js/counterup/jquery.counterup.min.js")"></script>

        <script src="@Url.Content("~/Content/js/jmask.js")"></script>
