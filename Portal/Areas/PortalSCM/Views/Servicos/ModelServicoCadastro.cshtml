<style>
    .uploadimagemsistema {
        display: none !important;
    }

    .newbtn {
        cursor: pointer;
    }
</style>

<div class="modal fade" id="SCMCadastroServico" role="dialog">
    <div class="modal-dialog modals-default">
        <div class="modal-content">
            <div class="modal-header" style="margin-bottom: 20px;background-color: #fff !important;">
                <div class="row">
                    <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
                        <div class="breadcomb-wp">
                            <div class="breadcomb-icon">
                                <i class="notika-icon notika-app"></i>
                            </div>
                            <div class="breadcomb-ctn">
                                <h2>Serviços</h2>
                                <p>Cadastro de Servços</p>
                            </div>
                        </div>
                    </div>
                </div>
                <button type="button" class="close" data-dismiss="modal">&times;</button>
            </div>
            <form id="SCMCadastroServico" method="get">
                <div class="modal-body">
                    <input id="ser_id" name="ser_id" type="hidden" value="" />
                    <div class="col-lg-6 col-md-12 col-sm-12 col-xs-12">
                        <div class="form-group">
                            <label for="mod_nome" class="control-label mb-1">Codigo Serviço</label>
                            <div class="nk-int-st">
                                <input id="ser_codigoServico" placeholder="Valor" name="ser_codigoServico" type="text" value="" class="form-control obrigatorio">
                            </div>

                        </div>
                    </div>
                    <div class="col-lg-6 col-md-12 col-sm-12 col-xs-12">
                        <div class="form-group">
                            <label for="mod_nome" class="control-label mb-1">Codigo Atividade</label>
                            <div class="nk-int-st">
                                <input id="ser_codigoAtividade" placeholder="Valor" name="ser_codigoAtividade" type="text" value="" class="form-control obrigatorio">
                            </div>

                        </div>
                    </div>
                    <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
                        <div class="form-group">
                            <label for="mod_nome" class="control-label mb-1">Descrição</label>
                            <div class="nk-int-st">
                                <input id="ser_descricaoServico" placeholder="Valor" name="ser_descricaoServico" type="text" value="" class="form-control obrigatorio">
                            </div>

                        </div>
                    </div>
                    <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
                        <div class="form-group">
                            <label for="mod_nome" class="control-label mb-1">Aliquota</label>
                            <div class="nk-int-st">
                                <input id="ser_aliquota" placeholder="Valor" name="ser_aliquota" type="text" value="" class="form-control obrigatorio">
                            </div>

                        </div>
                    </div>



                </div>
                <div class="modal-footer" style="margin-top: 30px">
                    <button type="button" class="btn btn-default" onclick="SCMCadastrarServicos()" >Salvar</button>
                    <button type="button" class="btn btn-default" data-dismiss="modal">Cancelar</button>
                </div>
            </form>
        </div>
    </div>
</div>